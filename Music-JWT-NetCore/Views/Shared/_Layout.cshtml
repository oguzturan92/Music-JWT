    @await RenderSectionAsync("Scripts", required: false)

    <script>
        $('#login').on('click', async function(){
            Login();
        });

        async function Login() {
            const { value: formValues } = await Swal.fire({
                title: "Giriş Yap",
                html: `
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">E-mail</label>
                        <input class="form-control" type="text" id="swal-input1">
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">Şifre</label>
                        <input class="form-control" type="password" id="swal-input2">
                    </div>
                `,
                focusConfirm: false,
                confirmButtonText: `Giriş Yap`,
                showCancelButton: true,
                cancelButtonText: `İptal`,
                preConfirm: () => {
                    return [
                        document.getElementById("swal-input1").value,
                        document.getElementById("swal-input2").value
                    ];
                }
            });
            if (formValues) {
                let loginDto = {
                    Email: formValues[0],
                    Password: formValues[1]
                };
                
                $.ajax({
                    type: "post",
                    url: "/Logins/Index/",
                    data: loginDto,
                    success: function (func) {
                        console.log(func);
                        if (func == 200)
                        {
                            Swal.fire({
                                position: "center",
                                icon: "success",
                                html: "Giriş Başarılı",
                                showConfirmButton: false,
                                timer: 1500
                            });
                            setTimeout(function() {
                                window.location.href = "/";
                            }, 1500);
                        }
                    }
                });
            }
        }

        $('#register').on('click', async function(){
            const { value: formValues } = await Swal.fire({
                title: "Kayıt Ol",
                html: `
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">Kullanıcı Adı</label>
                        <input class="form-control" type="text" id="swal-input1">
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">E-mail</label>
                        <input class="form-control" type="text" id="swal-input2">
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">Şifre</label>
                        <input class="form-control" type="password" id="swal-input3">
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">Şifre Tekrar</label>
                        <input class="form-control" type="password" id="swal-input4">
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size: 14px;">Premium</label>
                        <select id="swal-input5" class="form-control">
                            <option value="1">EliteCraft Premium</option>
                            <option value="2">PlatinumPlus Premium</option>
                            <option value="3">RoyalEdge Premium</option>
                            <option value="4">SupremeLine Premium</option>
                        </select>
                    </div>
                `,
                focusConfirm: false,
                confirmButtonText: `Kayıt Ol`,
                showCancelButton: true,
                cancelButtonText: `İptal`,
                preConfirm: () => {
                    return [
                        document.getElementById("swal-input1").value,
                        document.getElementById("swal-input2").value,
                        document.getElementById("swal-input3").value,
                        document.getElementById("swal-input4").value,
                        document.getElementById("swal-input5").value
                    ];
                }
            });
            if (formValues) {
                let registerDto = {
                    UserName: formValues[0],
                    Email: formValues[1],
                    Password: formValues[2],
                    RePassword: formValues[3],
                    PremiumId: formValues[4],
                };
                
                $.ajax({
                    type: "post",
                    url: "/Registers/Index/",
                    data: registerDto,
                    success: function (func) {
                        console.log(func);
                        if (func == 201)
                        {
                            Swal.fire({
                                position: "center",
                                icon: "success",
                                html: "Kayıt Başarılı",
                                showConfirmButton: false,
                                timer: 1500
                            });
                            setTimeout(function() {
                                window.location.href = "/";
                            }, 1500);
                        }
                    }
                });
            }
        });

        $('.player_button').each(async function(){
            $(this).on('click', async function(){
                $.ajax({
                    type: "get",
                    url: "/Home/Query/",
                    success: function (func1) {
                        console.log(func1);
                        if (func1 != 200)
                        {
                            $('#music-list').html("");
                            $('#music-title').html("");
                            $('#music-title').html(`<h2>Müzik Listesini Görüntülemek İçin<span id='music-login' style='text-decoration: underline; cursor: pointer;'>Giriş Yapınız</span></h2>`);
                            Login();
                            $('#music-login').on('click', async function(){
                                Login();
                            });
                        }
                    }
                });
            });
        });

        $('#music-login').on('click', async function(){
            Login();
        });

    </script>
